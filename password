    ; Suponemos que el password se encuentra almacenado en memoria y que cada dígito ocupa un byte consecutivo a partir de una dirección base específica (por ejemplo, la dirección base está en r1)

control_acceso:
    mov r2, #0        ; Contador de intentos fallidos

solicitar_password:
    mov r0, #0        ; Limpiar el registro r0
    mov r7, #4        ; Syscall para imprimir mensaje
    ldr r1, =mensaje_solicitud_password
    mov r2, #20       ; Longitud del mensaje
    mov r7, #4        ; Syscall para imprimir mensaje
    svc 0             ; Llamar al sistema

    mov r2, #0        ; Contador de dígitos ingresados

leer_digito:
    mov r7, #3        ; Syscall para leer un carácter
    svc 0             ; Llamar al sistema

    cmp r0, #10       ; Verificar si se ingresó un dígito válido (0-9)
    blt validar_digito
    b fin_control_acceso

validar_digito:
    cmp r2, #5        ; Verificar si se ingresaron los 5 dígitos del password
    beq verificar_password

    strb r0, [r1, r2] ; Almacenar el dígito ingresado en la memoria
    add r2, r2, #1    ; Incrementar el contador de dígitos ingresados

    mov r0, #42       ; Imprimir asterisco (*)
    mov r7, #4        ; Syscall para imprimir mensaje
    ldr r1, =asterisco
    mov r2, #1        ; Longitud del mensaje
    svc 0             ; Llamar al sistema

    b leer_digito

verificar_password:
    ldr r1, =clave_password ; Dirección de memoria donde se encuentra almacenada la clave de 5 dígitos
    ldr r3, [r1]      ; Cargar el primer dígito de la clave

    mov r4, #0        ; Contador de dígitos coincidentes

comparar_digitos:
    ldrb r5, [r1, r4] ; Cargar el dígito de la clave
    cmp r5, [r1, r4]  ; Comparar el dígito ingresado con el de la clave
    bne password_incorrecto

    add r4, r4, #1    ; Incrementar el contador de dígitos coincidentes

    cmp r4, #5        ; Verificar si se compararon los 5 dígitos
    beq password_correcto

    add r1, r1, #1    ; Avanzar a la siguiente posición de memoria de la clave
    b comparar_digitos

password_correcto:
    mov r0, #0        ; Limpiar el registro r0
    mov r7, #4        ; Syscall para imprimir mensaje
    ldr r1, =mensaje_bienvenida
    mov r2, #19       ; Longitud del mensaje
    svc 0             ; Llamar al sistema

    b fin_control_acceso

password_incorrecto:
    add r2, r2, #1    ; Incrementar el contador de intentos fallidos

    cmp r2, #3        ; Verificar si se han realizado 3 intentos fallidos
    beq abortar_programa

    b solicitar_password

abortar_programa:
    mov r0, #0        ; Limpiar el registro r0
    mov r7, #4        ; Syscall para imprimir mensaje
    ldr r1, =mensaje_password_invalida
    mov r2, #17       ; Longitud del mensaje
    svc 0             ; Llamar al sistema

fin_control_acceso:
    ; Continuación del programa después del control de acceso
    ; ...
